Include ~/.orbstack/ssh/config

#############
## Devices ##
#############
%{ for host, device in devices ~}

Host ${host}
	HostName ${host}
	Port ${device.port}
	User ${device.username}
%{ endfor ~}

#############
## Servers ##
#############
%{ for host, server in servers ~}

Host ${host}
	HostName ${host}
	Port ${server.networks[0].ssh_port}
	User ${server.users[0].username}
%{ endfor ~}
